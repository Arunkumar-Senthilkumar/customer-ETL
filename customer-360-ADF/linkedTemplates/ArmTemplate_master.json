					},
					"ls_sqlserver_properties_typeProperties_database": {
						"value": "[parameters('ls_sqlserver_properties_typeProperties_database')]"
					},
					"ls_sqlserver_properties_typeProperties_userName": {
						"value": "[parameters('ls_sqlserver_properties_typeProperties_userName')]"
					},
					"ls_synapse_properties_typeProperties_server": {
						"value": "[parameters('ls_synapse_properties_typeProperties_server')]"
					},
					"ls_synapse_properties_typeProperties_database": {
						"value": "[parameters('ls_synapse_properties_typeProperties_database')]"
					},
					"ls_synapse_properties_typeProperties_userName": {
						"value": "[parameters('ls_synapse_properties_typeProperties_userName')]"
					},
					"ls_backenddataadls_properties_typeProperties_url": {
						"value": "[parameters('ls_backenddataadls_properties_typeProperties_url')]"
					},
					"ls_backenddataadls_properties_typeProperties_tenant": {
						"value": "[parameters('ls_backenddataadls_properties_typeProperties_tenant')]"
					},
					"ls_backenddataadls_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ls_backenddataadls_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}